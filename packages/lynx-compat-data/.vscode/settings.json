{
  "sortJSON.orderOverride": ["version_added", "version_removed"],
  "sortJSON.orderUnderride": ["status", "standard_track", "deprecated"],
  "json.schemas": [
    {
      "fileMatch": [
        "/lynx-api/*.json",
        "/lynx-native-api/*.json",
        "/css/*.json",
        "/react/*.json"
      ],
      "url": "/schemas/compat-data.schema.json"
    },
    {
      "fileMatch": [
        "/platforms/*.json",
        "!/platforms/platforms.json",
        "!/platforms/platform-keys.json"
      ],
      "url": "/schemas/platform.schema.json"
    }
  ]
}
